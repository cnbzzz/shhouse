  <select id="listHouseQtByHouseId" parameterType="String" resultType="cn.jufangbao.house.shhouse.dto.HouseQtDto">
  SELECT
      t1.*, t2.sclazzname, t3.clazzname, t4.dictname
  FROM
      t_res_house_qt t1,
      t_res_house_smclazz t2,
      t_res_house_clazz t3,
      t_sa_dict_item t4
  WHERE
      t1.smclazoid = t2.oid
  AND t1.clazoid = t3.oid
  AND t1.qttype = t4.dictid
  AND t4.groupid = 'HOUSE_QT_TYPE'
  AND t1.`status` = 1
  AND t2.`status` = 1
  AND t3.`status` = 1
  AND t4.`status` = 1
  AND t1.hoid = #{oid,jdbcType=BIGINT}
  ORDER BY t1.sort
  </select>
